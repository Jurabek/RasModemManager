<Window x:Class="RasModemManager.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <ComboBox HorizontalAlignment="Center" Name="uiComboBox" VerticalAlignment="Top" Width="318" Height="30" ItemsSource="{Binding Path=EntryNames}" SelectedItem="{Binding Path=SelectedEntryName}"/>
        <Button Content="Connect" HorizontalAlignment="Left" Margin="100,35,0,0" VerticalAlignment="Top" Width="318" Height="30" Click="Button_Click"/>
        <Button Content="Disconnect" HorizontalAlignment="Left" Margin="100,84,0,0" VerticalAlignment="Top" Width="318" Height="34" Click="Button_Click_1"/>

    </Grid>
</Window>
